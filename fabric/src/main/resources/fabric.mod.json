{
  "schemaVersion": 1,
  "id": "cc_hexed",
  "version": "${version}",
  "name": "CC: Hexed",
  "description": "Hexcasting x CC: Tweaked Interop",
  "authors": [
    "TechTastic"
  ],
  "contact": {
    "homepage": "https://techtastic.github.io/CC-Hexed",
    "sources": "https://github.com/TechTastic/CC-Hexed"
  },
  "license": "MIT",
  "icon": "assets/cc_hexed/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [{
      "adapter": "kotlin",
      "value": "io.github.techtastic.cc_hexed.fabric.FabricCCHexed"
    }],
    "client": [{
      "adapter": "kotlin",
      "value": "io.github.techtastic.cc_hexed.fabric.FabricCCHexedClient"
    }],
    "server": [{
      "adapter": "kotlin",
      "value": "io.github.techtastic.cc_hexed.fabric.FabricCCHexedServer"
    }],
    "fabric-datagen": [{
      "adapter": "kotlin",
      "value": "io.github.techtastic.cc_hexed.fabric.datagen.FabricCCHexedDatagen"
    }],
    "modmenu": [{
      "adapter": "kotlin",
      "value": "io.github.techtastic.cc_hexed.fabric.FabricCCHexedModMenu"
    }]
  },
  "mixins": [
    "cc_hexed-common.mixins.json",
    "cc_hexed-fabric.mixins.json"
  ],
  "depends": {
    "minecraft": "${versions.minecraft}",
    "fabricloader": ">=${versions.fabric_loader}",
    "fabric-api": ">=${versions.fabric_api}",
    "fabric-language-kotlin": ">=${versions.kotlin_fabric}",
    "architectury": ">=${versions.architectury}",
    "hexcasting": ">=${versions.hexcasting}",
    "cloth-config": ">=${versions.clothConfig}"
  },
  "suggests": {
    "modmenu": ">=${versions.modMenu}"
  }
}
